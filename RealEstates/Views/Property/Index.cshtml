@model IEnumerable<RealEstates.Models.Property>

@{ ViewData["Title"] = "Property"; }

<br />
<p>
    <a asp-action="Create" type="button" class="btn btn-info">Add New Property</a>
</p>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TypeOfProperty)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dimentions)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.State)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.City)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PinCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.TypeOfProperty)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Dimentions)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.State)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.City)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.PinCode)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Rate)
    </td>
    <td>
        <a asp-action="Edit" asp-route-id="@item.Id" type="button" class="btn btn-info">Edit</a>
        <a asp-action="Details" asp-route-id="@item.Id" type="button" class="btn btn-info">Details</a>
        <a asp-action="Delete" asp-route-id="@item.Id" type="button" class="btn btn-info">Delete</a>
    </td>
</tr>
}
    </tbody>
</table>
<div>
    <a asp-controller="Logins" asp-action="Zone" type="button" class="btn btn-info">Back To Home</a>
</div>

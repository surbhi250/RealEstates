@model IEnumerable<RealEstates.Models.Property>

@{ ViewData["Title"] = "Property"; }

<br />
<h1>Available Properties</h1>
<br />
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TypeOfProperty)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dimentions)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.State)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.City)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PinCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rate)
            </th>
            <th>
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.TypeOfProperty)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Dimentions)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.State)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.City)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.PinCode)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Rate)
    </td>
</tr>}
    </tbody>
</table>
@model IEnumerable<RealEstates.Models.BuyProperty>

@{
    ViewData["Title"] = "Index";
}
<br />
<p>
    <a asp-action="Create" type="button" class="btn btn-info">Buy New Property</a>
</p>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contact)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PropertyId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dimentions)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.State)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.City)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PinCode)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Contact)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.PropertyId)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Dimentions)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.State)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.City)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.PinCode)
    </td>
    <td>
        <a asp-action="Edit" asp-route-id="@item.Id" type="button" class="btn btn-info">Edit</a>
        <a asp-action="Details" asp-route-id="@item.Id" type="button" class="btn btn-info">Details</a>
        <a asp-action="Delete" asp-route-id="@item.Id" type="button" class="btn btn-info">Delete</a>
    </td>
</tr>}
    </tbody>
</table>
